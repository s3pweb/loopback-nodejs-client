{
  "name": "loopback-nodejs-client",
  "version": "1.5.6",
  "description": "",
  "main": "src/index.js",
  "files": [
    "bin/",
    "src/"
  ],
  "ava": {
    "require": [
      "ava-playback"
    ],
    "playbacks": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "test": "NOCK=play node_modules/.bin/ava -s",
    "postbuild": "npm run test -s",
    "coverage": "node_modules/.bin/nyc npm test && node_modules/.bin/nyc report --reporter=html",
    "coveralls": "node_modules/.bin/nyc npm test && node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls",
    "postcoveralls": "rimraf ./coverage",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sguilly/loopback-nodejs-client"
  },
  "keywords": [
    "nodejs loopback rest api client"
  ],
  "author": "St√©phane GUILLY <stephane.guilly@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sguilly/loopback-nodejs-client/issues"
  },
  "homepage": "https://github.com/sguilly/loopback-nodejs-client#readme",
  "devDependencies": {
    "ava": "5.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.13.3",
    "dependency-check": "^2.5.1",
    "doctoc": "^0.15.0",
    "eslint": ">=4.18.2",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "nyc": "^12.0.2",
    "watch": "^0.16.0",
    "ava-playback": "0.0.14",
    "nock": "^13.2.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "node-fetch": "^2.6.7",
    "qs": "^6.10.3"
  }
}
